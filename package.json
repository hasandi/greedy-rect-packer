  "description": "Rectangle packer using greedy algorithm.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src --ext .js,.ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
